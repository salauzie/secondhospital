<%= form_for @clinic do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<br>
	<%= f.label :facility_type %>
	<%= f.text_field :facility_type %>
	<br>
	<%= f.label :address %>
  <%= f.text_field :address %>
  <br>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <br>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <br>
  <%= f.submit %>
  <% end %>